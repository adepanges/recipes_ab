# EditorConfig https://EditorConfig.org
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.js]
indent_style = space
indent_size = 4

[*.md]
indent_style = space
indent_size = 4

[{package.json,.eslintrc.json}]
indent_style = space
indent_size = 4